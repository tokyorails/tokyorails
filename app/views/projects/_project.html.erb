<div class="row">
  <h2><%= project.title %></h3>
  <p><%= project.description %></p>
  <p><%= link_to project.github_url, project.github_url %></p>
  <ul class="media-grid">
    <li>
    <%= image_tag project.photo_url, :width => 80 %>
    </li>
  </ul>
  <div class="embed_html">
    <%= project.html.try(:html_safe) %>
  </div>
</div>
